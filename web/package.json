{
    "name": "web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:prod": "vite --mode prod",
        "dev:sta": "vite --mode sta",
        "dev:dev": "vite --mode dev",
        "build": "vue-tsc --noEmit && vite build",
        "build:prod": "vue-tsc --noEmit && vite build --mode prod",
        "build:sta": "vue-tsc --noEmit && vite build --mode sta",
        "build:dev": "vue-tsc --noEmit && vite build --mode dev",
        "revision": "echo $GITHUB_SHA > ./public/revision",
        "preview": "vite preview",
        "prettier": "prettier --write .",
        "generate": "graphql-codegen --config codegen.yml",
        "ts:download": "CROWDIN_TOKEN=`cat .crowdin_token` && export CROWDIN_TOKEN && crowdin download",
        "ts:upload": "CROWDIN_TOKEN=`cat .crowdin_token` && export CROWDIN_TOKEN && crowdin upload"
    },
    "dependencies": {
        "@auth0/auth0-vue": "^2.1.0",
        "@headlessui/vue": "^1.7.12",
        "@heroicons/vue": "^2.0.16",
        "@sentry/tracing": "^7.45.0",
        "@sentry/vue": "^7.45.0",
        "@takuma-ru/vue-swipe-modal": "^4.0.7",
        "@types/showdown": "^2.0.0",
        "@urql/exchange-auth": "^2.0.0",
        "@urql/vue": "1.0.4",
        "bccm-video-player": "1.4.0-test",
        "graphql": "^16.6.0",
        "graphql-tag": "^2.12.6",
        "rudder-sdk-js": "^2.28.0",
        "showdown": "^2.1.0",
        "swiper": "^9.1.1",
        "vue": "^3.2.47",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.6",
        "vue3-lottie": "^2.5.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@graphql-codegen/cli": "^3.2.2",
        "@graphql-codegen/typescript": "^3.0.2",
        "@graphql-codegen/typescript-operations": "^3.0.2",
        "@graphql-codegen/typescript-vue-urql": "^2.3.6",
        "@intlify/unplugin-vue-i18n": "^0.10.0",
        "@types/node": "^18.15.11",
        "@vitejs/plugin-vue": "^4.1.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.37.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier-vue": "^4.2.0",
        "eslint-plugin-vue": "^9.10.0",
        "postcss": "^8.4.21",
        "prettier": "^2.8.7",
        "tailwindcss": "^3.3.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2",
        "vite": "^4.2.1",
        "vue-tsc": "^1.2.0"
    }
}
