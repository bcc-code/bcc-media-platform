{
    "name": "web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:prod": "vite --mode prod",
        "dev:sta": "vite --mode sta",
        "dev:dev": "vite --mode dev",
        "build": "vue-tsc --noEmit && vite build",
        "build:prod": "vue-tsc --noEmit && vite build --mode prod",
        "build:sta": "vue-tsc --noEmit && vite build --mode sta",
        "build:dev": "vue-tsc --noEmit && vite build --mode dev",
        "revision": "echo $GITHUB_SHA > ./public/revision",
        "preview": "vite preview",
        "prettier": "prettier --write .",
        "generate": "graphql-codegen --config codegen.yml",
        "ts:download": "CROWDIN_TOKEN=`cat .crowdin_token` && export CROWDIN_TOKEN && crowdin download",
        "ts:upload": "CROWDIN_TOKEN=`cat .crowdin_token` && export CROWDIN_TOKEN && crowdin upload"
    },
    "dependencies": {
        "@auth0/auth0-vue": "^2.2.0",
        "@headlessui/vue": "^1.7.14",
        "@heroicons/vue": "^2.0.18",
        "@sentry/tracing": "^7.56.0",
        "@sentry/vue": "^7.56.0",
        "@takuma-ru/vue-swipe-modal": "^4.0.7",
        "@types/showdown": "^2.0.1",
        "@urql/exchange-auth": "^2.0.0",
        "@urql/vue": "1.0.4",
        "bccm-video-player": "1.4.0-test",
        "graphql": "^16.6.0",
        "graphql-tag": "^2.12.6",
        "rudder-sdk-js": "^2.35.0",
        "showdown": "^2.1.0",
        "swiper": "^9.4.1",
        "vue": "^3.3.4",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.2.2",
        "vue3-lottie": "^2.7.1"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^3.2.2",
        "@graphql-codegen/typescript": "^3.0.2",
        "@graphql-codegen/typescript-operations": "^3.0.2",
        "@graphql-codegen/typescript-vue-urql": "^2.3.6",
        "@intlify/unplugin-vue-i18n": "^0.11.0",
        "@tailwindcss/typography": "^0.5.9",
        "@types/node": "^20.3.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier-vue": "^4.2.0",
        "eslint-plugin-vue": "^9.15.0",
        "postcss": "^8.4.24",
        "prettier": "^2.8.8",
        "tailwindcss": "^3.3.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3",
        "vite": "^4.3.9",
        "vue-tsc": "^1.8.0"
    }
}
