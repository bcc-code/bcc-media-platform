{
    "name": "web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:prod": "vite --mode prod",
        "dev:sta": "vite --mode sta",
        "dev:dev": "vite --mode dev",
        "build": "vue-tsc --noEmit && vite build",
        "build:prod": "vue-tsc --noEmit && vite build --mode prod",
        "build:sta": "vue-tsc --noEmit && vite build --mode sta",
        "build:dev": "vue-tsc --noEmit && vite build --mode dev",
        "revision": "echo $GITHUB_SHA > ./public/revision",
        "preview": "vite preview",
        "prettier": "prettier --write .",
        "generate": "graphql-codegen --config codegen.yml",
        "crowdin:download": "CROWDIN_TOKEN=`cat .crowdin_token` && export CROWDIN_TOKEN && crowdin download",
        "crowdin:upload": "CROWDIN_TOKEN=`cat .crowdin_token` && export CROWDIN_TOKEN && crowdin upload",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "@auth0/auth0-vue": "^2.4.0",
        "@bcc-code/bmm-sdk-fetch": "8.8.8",
        "@headlessui/vue": "^1.7.23",
        "@heroicons/vue": "^2.2.0",
        "@sentry/browser": "^8.44.0",
        "@sentry/tracing": "^7.114.0",
        "@sentry/vue": "^8.44.0",
        "@takuma-ru/vue-swipe-modal": "^5.0.5",
        "@types/showdown": "^2.0.6",
        "@unleash/proxy-client-vue": "^0.1.5",
        "@urql/exchange-auth": "2.2.1",
        "@urql/vue": "1.4.3",
        "@vueuse/core": "^13.5.0",
        "@vueuse/motion": "^2.2.6",
        "bccm-video-player": "1.6.0",
        "graphql": "^16.11.0",
        "graphql-tag": "^2.12.6",
        "rudder-sdk-js": "^2.51.4",
        "showdown": "^2.1.0",
        "swiper": "^11.2.10",
        "video.js": "^7.21.6",
        "vue": "^3.5.17",
        "vue-i18n": "^10.0.8",
        "vue-router": "^4.5.1",
        "vue3-lottie": "^3.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@graphql-codegen/cli": "5.0.7",
        "@graphql-codegen/typescript": "4.1.6",
        "@graphql-codegen/typescript-operations": "4.6.1",
        "@graphql-codegen/typescript-vue-urql": "3.2.1",
        "@intlify/unplugin-vue-i18n": "^4.0.0",
        "@tailwindcss/typography": "^0.5.16",
        "@types/node": "^20.17.10",
        "@types/video.js": "^7.3.58",
        "@typescript-eslint/parser": "^8.43.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/tsconfig": "^0.8.1",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-unused-imports": "^4.2.0",
        "eslint-plugin-vue": "^10.4.0",
        "globals": "^16.3.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "tailwindcss": "^3.4.16",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.43.0",
        "vite": "^6.0.0",
        "vue-tsc": "^3.0.6"
    },
    "packageManager": "pnpm@9.7.1"
}
