{
    "name": "web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:prod": "vite --mode prod",
        "dev:sta": "vite --mode sta",
        "dev:dev": "vite --mode dev",
        "build": "vue-tsc --noEmit && vite build",
        "build:prod": "vue-tsc --noEmit && vite build --mode prod",
        "build:sta": "vue-tsc --noEmit && vite build --mode sta",
        "build:dev": "vue-tsc --noEmit && vite build --mode dev",
        "revision": "echo $GITHUB_SHA > ./public/revision",
        "preview": "vite preview",
        "prettier": "prettier --write .",
        "generate": "graphql-codegen --config codegen.yml",
        "crowdin:download": "CROWDIN_TOKEN=`cat .crowdin_token` && export CROWDIN_TOKEN && crowdin download",
        "crowdin:upload": "CROWDIN_TOKEN=`cat .crowdin_token` && export CROWDIN_TOKEN && crowdin upload",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "@auth0/auth0-vue": "^2.3.3",
        "@bcc-code/bmm-sdk-fetch": "8.8.0",
        "@headlessui/vue": "^1.7.23",
        "@heroicons/vue": "^2.2.0",
        "@sentry/browser": "^8.44.0",
        "@sentry/tracing": "^7.114.0",
        "@sentry/vue": "^8.44.0",
        "@takuma-ru/vue-swipe-modal": "^5.0.5",
        "@types/showdown": "^2.0.6",
        "@unleash/proxy-client-vue": "^0.1.2",
        "@urql/exchange-auth": "2.2.0",
        "@urql/vue": "1.3.2",
        "@vueuse/core": "^10.11.1",
        "@vueuse/motion": "^2.2.6",
        "bccm-video-player": "1.5.7",
        "graphql": "^16.9.0",
        "graphql-tag": "^2.12.6",
        "rudder-sdk-js": "^2.48.41",
        "showdown": "^2.1.0",
        "swiper": "^11.1.15",
        "video.js": "^7.21.6",
        "vue": "^3.5.13",
        "vue-i18n": "^10.0.5",
        "vue-router": "^4.5.0",
        "vue3-lottie": "^3.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.16.0",
        "@graphql-codegen/cli": "5.0.2",
        "@graphql-codegen/typescript": "4.0.7",
        "@graphql-codegen/typescript-operations": "4.2.1",
        "@graphql-codegen/typescript-vue-urql": "3.1.0",
        "@intlify/unplugin-vue-i18n": "^4.0.0",
        "@tailwindcss/typography": "^0.5.15",
        "@types/node": "^20.17.10",
        "@types/video.js": "^7.3.58",
        "@typescript-eslint/parser": "^8.18.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "eslint-plugin-vue": "^9.32.0",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "tailwindcss": "^3.4.16",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.0",
        "vite": "^5.4.11",
        "vue-tsc": "https://pkg.pr.new/vuejs/language-tools/vue-tsc@3fb59af"
    },
    "packageManager": "pnpm@9.7.1"
}
