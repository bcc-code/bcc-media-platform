{
    "name": "bccm-video-player",
    "version": "1.5.0",
    "type": "commonjs",
    "types": "build/types",
    "files": [
        "build"
    ],
    "exports": {
        ".": {
            "import": "./build/bccm-video-player.mjs",
            "require": "./build/bccm-video-player.umd.js"
        },
        "./css": "./build/style.css"
    },
    "scripts": {
        "dev": "vite",
        "types": "tsc --emitDeclarationOnly && mv build/src build/types && rm -r build/external-projects",
        "build": "tsc --noEmit && vite build && pnpm types",
        "preview": "vite preview",
        "prettier": "prettier --write ."
    },
    "dependencies": {
        "@silvermine/videojs-chromecast": "^1.4.1",
        "graphql": "^16.6.0",
        "graphql-request": "^5.2.0",
        "ua-parser-js": "^1.0.34",
        "video.js": "^7.21.4",
        "videojs-contrib-quality-levels": "^2.2.1",
        "videojs-event-tracking": "^1.0.3",
        "videojs-mux": "^4.15.0",
        "youbora-adapter-videojs": "^6.8.12",
        "youboralib": "^6.8.46"
    },
    "devDependencies": {
        "@types/node": "^18.15.11",
        "@types/ua-parser-js": "^0.7.36",
        "@types/video.js": "^7.3.51",
        "@vitejs/plugin-vue": "^3.2.0",
        "postcss": "^8.4.21",
        "prettier": "^2.8.7",
        "sass": "^1.60.0",
        "typescript": "^4.9.5",
        "vite": "^3.2.5",
        "vue": "^3.2.47",
        "vue-tsc": "^0.40.13"
    }
}
