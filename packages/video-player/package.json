{
    "name": "bccm-video-player",
    "version": "1.3.4",
    "type": "commonjs",
    "types": "build/types",
    "files": [
        "build"
    ],
    "exports": {
        ".": {
            "import": "./build/bccm-video-player.mjs",
            "require": "./build/bccm-video-player.umd.js"
        },
        "./css": "./build/style.css"
    },
    "scripts": {
        "dev": "vite",
        "types": "tsc --emitDeclarationOnly && mv build/src build/types && rm -r build/external-projects",
        "build": "tsc --noEmit && vite build && pnpm types",
        "preview": "vite preview",
        "prettier": "prettier --write ."
    },
    "dependencies": {
        "graphql": "^16.6.0",
        "graphql-request": "^5.2.0",
        "ua-parser-js": "^1.0.33",
        "video.js": "^7.21.3",
        "videojs-contrib-quality-levels": "^2.2.1",
        "videojs-event-tracking": "^1.0.3",
        "videojs-mux": "^4.13.4",
        "youbora-adapter-videojs": "^6.8.12",
        "youboralib": "^6.8.42"
    },
    "devDependencies": {
        "@types/node": "^18.14.4",
        "@types/ua-parser-js": "^0.7.36",
        "@types/video.js": "^7.3.51",
        "@vitejs/plugin-vue": "^3.2.0",
        "postcss": "^8.4.21",
        "prettier": "^2.8.4",
        "sass": "^1.58.3",
        "typescript": "^4.9.5",
        "vite": "^3.2.5",
        "vue-tsc": "^0.40.13",
        "vue": "^3.2.47"
    }
}
